<?xml version="1.0" encoding="UTF-8"?>
<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="$APP_NAME"
                location="applications"/>
  <shortcut
    name="$APP_NAME"
    target="javaw"
    commandLine="-jar $INSTALL_PATH\wremja-launcher.jar"
    workingDirectory="$INSTALL_PATH"
    description="Zeiterfassung fuer Projekte."
    iconFile="$INSTALL_PATH\Wremja-Tray.ico"
    iconIndex="0"
    initialState="maximized"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="yes"
    type="Application"
    >

    <createForPack name="Base"/>
  </shortcut>
</shortcuts>
